{
    "collectCoverage": true,
    "collectCoverageFrom": [
        "**/src/**.{js,jsx}",
        "!**/src/playground/**",
        "!**/src/tests/**"
    ],
    "setupFiles": [
        "raf/polyfill",
        "<rootDir>/src/tests/setupTests.js"
    ],
    "snapshotSerializers": [
        "enzyme-to-json/serializer"
    ]
}